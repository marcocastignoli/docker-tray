{
  "name": "docker-tray",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "electron-builder .",
    "start": "APP_DEV=true electron ."
  },
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "^21.2.0"
  },
  "build": {
    "appId": "marcocastignoli.dockertray",
    "extraFiles": [
      {
        "from": "resources",
        "to": "resources",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "dependencies": {
    "node-docker-api": "^1.1.22"
  }
}
